{
  "manifest_version": 3,
  "name": "Replace Image Href for school.mos.ru",
  "version": "1.1",
  "description": "Заменяет href атрибутов <image> с определёнными clip-path на заданную ссылку на сайте school.mos.ru.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://school.mos.ru/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["KNOWN_IDS.txt"],
      "matches": ["https://school.mos.ru/*"]
    }
  ]
}
